{
  "manifest_version": 3,
  "name": "Procspy Extension",
  "version": "1.0",
  "description": "Automated Proctoring System Extension",
  "author": "Reza1290",
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Start Procspy"
  },
  "commands": {
    "_execute_action": {  
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "host_permissions": [
    "http://*/",
    "https://*/*",
    "https://192.168.2.5:5050/",
    "<all_urls>",
    "chrome-extension://"
    
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "scripts/procspy.js",
        "page/ask.html"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "sidePanel",
    "scripting",
    "tabs",
    "activeTab",
    "history",
    "system.cpu",
    "system.memory",
    "system.display",
    "system.storage",
    "notifications",
    "cookies",
    "storage",
    "desktopCapture",
    "gcm"

  ],
  "icons": {
    "16": "assets/images/icon-16.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  }
}